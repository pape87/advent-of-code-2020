{
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "eslint-plugin-import"
    ],
    "env": {
      "browser": true
    },
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
      "project": [],
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "@typescript-eslint/explicit-function-return-type": "off",
      "no-console": 2,
      "@typescript-eslint/no-unused-vars": "warn",
      "quotes": "off",
      "@typescript-eslint/quotes": [
        "warn",
        "double"
      ],
      "semi" : [2, "always"]
    },
    "settings": {
    }
  }